{
  "name": "telegram-checkers-mini-app",
  "version": "1.0.0",
  "description": "Telegram Mini App for Checkers Game",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && nodemon server.js",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run build && npx serve -s dist",
    "vercel-build": "cd frontend && npm run build"
  },
  "keywords": [
    "telegram",
    "mini-app",
    "checkers",
    "game"
  ],
  "author": "Telegram Checkers Mini App",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^7.6.0",
    "serve": "^14.2.0"
  },
  "engines": {
    "node": ">=16.x"
  }
}